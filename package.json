{
  "name": "@elizaos/plugin-tbb-ultimate",
  "version": "1.0.0",
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "scripts": {
    "start": "elizaos start",
    "dev": "vite",
    "build": "tsc --noEmit && vite build && tsup",
    "build:types": "tsup src/index.ts --format esm,cjs --dts --out-dir dist",
    "lint": "prettier --write ./src",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:install": "node scripts/install-test-deps.js",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "check-all": "npm run type-check && npm run format:check && npm run test",
    "cy:open": "npm run test:install && cypress open",
    "cy:run": "npm run test:install && cypress run --component",
    "cy:test": "npm run test:install && cypress run --component --reporter spec",
    "cypress:component": "npm run test:install && cypress run --component",
    "cypress:e2e": "npm run test:install && cypress run --e2e",
    "cypress:open": "npm run test:install && cypress open"
  },
  "dependencies": {
    "@elizaos/core": "^1.2.1",
    "@jest/globals": "^30.0.4",
    "@solana/web3.js": "^1.98.2",
    "@types/node": "^24.0.13",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "jest-environment-jsdom": "^30.0.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "technicalindicators": "^3.0.12"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/cypress": "^10.0.3",
    "@types/jest": "^30.0.0",
    "chalk": "^5.4.1",
    "cypress": "^14.4.1",
    "jest": "^30.0.4",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3"
  },
  "agentConfig": {
    "actions": [
      "swap",
      "buy",
      "sell",
      "checkWalletBalance",
      "pumpFunTrade",
      "createPumpFunToken",
      "getPumpFunData",
      "getDexscreenerData",
      "technicalAnalysis",
      "portfolioTrackerAction",
      "marketTrends",
      "sentimentAnalysis",
      "degenStrategy",
      "arbitrageStrategy",
      "safeStrategy",
      "daoStrategy",
      "startAutoBuySell",
      "stopAutoBuySell"
    ],
    "providers": [
      "solana",
      "jupiter",
      "dexscreener",
      "pumpfun",
      "orca",
      "raydium",
      "technicalindicators"
    ],
    "evaluators": [
      "predict"
    ],
    "models": [
      "gpt-4",
      "gpt-3.5-turbo",
      "claude-3"
    ],
    "services": [
      "priceMonitor",
      "autoBuySell"
    ]
  }
}
