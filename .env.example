# Your Solana wallet secret key as JSON array string
PRIVATE_KEY='[1,2,3,...,64]'

# Solana RPC URL
 **Recommended**: Use reliable providers like Helius, QuickNode, or Alchemy
SOLANA_RPC='https://api.mainnet-beta.solana.com' 
HELIUS/QUICKNODE/ALCHEMY_API_KEY=your_api_key
# CoinMarketCap API key (optional, used as price data fallback)
COINMARKETCAP_API_KEY='your_coinmarketcap_api_key_here'

# Enable or disable automated trading functionality
AUTO_TRADE_ENABLED='true'

# Interval in milliseconds for automatic buy/sell cycle
AUTO_BUY_SELL_INTERVAL_MS='60000'

# Minimum price spread ratio (e.g. 0.01 = 1%) for arbitrage to trigger trades
ARBITRAGE_SPREAD_THRESHOLD='0.01'

# Interval in milliseconds to poll arbitrage opportunities
ARBITRAGE_POLL_INTERVAL_MS='30000'

# Twitter API keys ****X(Twitter) is required for this plugin to function properly. Ai predict, sentimentent analysis and the prediction strategy rely on it***
TWITTER_API_KEY='your_twitter_api_key_here'
TWITTER_API_SECRET='your_twitter_api_secret_here'
TWITTER_ACCESS_TOKEN='your_twitter_access_token_here'
TWITTER_ACCESS_SECRET='your_twitter_access_secret_here'

# Telegram Bot Token and Chat ID for alerts (optional)
TELEGRAM_BOT_TOKEN='your_telegram_bot_token_here'
TELEGRAM_CHAT_ID='your_telegram_chat_id_here'